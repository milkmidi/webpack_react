{
  "name": "react_template",
  "version": "1.0.0",
  "description": "react-template",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --hot",
    "weinre": "set WEINRE=true && npm run start",
    "https": "cross-env NODE_ENV=development webpack-dev-server --progress --hot --https",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "plop": "plop --plopfile ./internal/generator/index.js",
    "flow": "flow",
    "jest": "jest",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "source": "dist",
    "skipThirdPartyRequests": true,
    "allowedThirdPartyDomains": [
      "https://cdnjs.cloudflare.com"
    ],
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "author": "milkmidi",
