{
  "presets": [
    "env",
    "es2015",
    "stage-3",
    "react",
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    "transform-runtime",
    "transform-function-bind",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "transform-async-generator-functions"
  ]
}